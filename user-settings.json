// Place your settings in this file to overwrite the default settings
{
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html",
    "plaintext": "jade"
  },
  "emmet.triggerExpansionOnTab": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "coverage": true,
    "USE_GITIGNORE": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "jspm_packages": true,
    "node_modules": true,
    "coverage": true,
    "USE_GITIGNORE": true
  },
  "editor.parameterHints.enabled": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "html.suggest.html5": false,
  "editor.snippetSuggestions": "none",
  "editor.fontFamily": "Hack",
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "javascript.validate.enable": false,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<C-n>"
      ],
      "after": [],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-d>": false
  },
  "files.autoSave": "onFocusChange",
  "window.zoomLevel": 2,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "python.pythonPath": "/usr/local/bin/python3",
  "beautify.tabSize": 2,
  "workbench.iconTheme": "vs-seti",
  "html.format.wrapAttributes": "force",
  "workbench.colorTheme": "Visual Studio Dark",
  "editor.lineNumbers": "on",
  "editor.parameterHints": false,
  "git.confirmSync": false,
  "vim.enableNeovim": true,
  "vim.disableAnnoyingNeovimMessage": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": true
  },
  "git.autofetch": true,
  "gitlens.keymap": "chorded",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "yaml.schemas": {
    "file:///toc.schema.json": "/toc\\.yml/i"
  },
  "python.dataScience.allowImportFromNotebook": false,
  "python.dataScience.jupyterLaunchRetries": 2,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "python.dataScience.jupyterLaunchTimeout": 5000,
  "latex-workshop.view.pdf.viewer": "tab",
  "aws.samcli.location": "/usr/local/bin/sam",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": false,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.lintTask.enable": true,
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescriptreact"
  ]
}
